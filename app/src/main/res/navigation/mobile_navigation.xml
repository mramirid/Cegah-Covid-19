<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mramirid.cegahcovid_19.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_checkup"
        android:name="com.mramirid.cegahcovid_19.ui.checkup.CheckupFragment"
        android:label="@string/checkup"
        tools:layout="@layout/fragment_checkup" />

    <fragment
        android:id="@+id/navigation_stats"
        android:name="com.mramirid.cegahcovid_19.ui.statistics.StatisticsFragment"
        android:label="@string/statistics"
        tools:layout="@layout/fragment_statistics" />
</navigation>
